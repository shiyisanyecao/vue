*
    margin 0
    padding 0
    box-sizing border-box

ul,li
    list-style none

.r-swiper-out
    position relative
    &-item
        padding 1em
        width 100%
        height 64px
        
        &-content
            width 100%
        &-btns
            width 100px
            position absolute
            overflow hidden
            right 0
            transform translateX(100%)